{
  "name": "nocalhost-server-cli",
  "version": "0.0.0",
  "private": true,
  "description": "nocalhost server cli",
  "main": "lib/main.js",
  "scripts": {
    "watch": "npm run compile -- --sourcemap --watch",
    "package": "npm run compile -- --minify",
    "compile": "esbuild ./src/main.ts ./src/post.ts --bundle --format=cjs --platform=node --target=node14 --outdir=dist",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts --fix",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "axios": "^0.26.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.17",
    "@typescript-eslint/parser": "^5.30.7",
    "esbuild": "^0.14.23",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.4"
  }
}
